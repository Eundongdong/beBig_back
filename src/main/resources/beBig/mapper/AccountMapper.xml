<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="beBig.mapper.AccountMapper">

    <select id="findAccountById" parameterType="String" resultType="AccountVo">
        SELECT *
        FROM account
        WHERE account_connected_id = #{connectedId}
    </select>

    <select id="getBankNameByCode" resultType="String">
        SELECT bank_name
        FROM bank
        WHERE bank_code_codef = #{bankCode}
    </select>
</mapper>
