<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="beBig.mapper.BankMapper">

    <select id="getBankIdByFssCode" parameterType="String" resultType="java.lang.Integer">
        SELECT bank_id FROM bank WHERE bank_code_fss = #{bankCodeFss}
    </select>


</mapper>
